<h1>Criar conta</h1>
<%= form_for @psicologo do |f|%>
   <% if @psicologo.errors.any? %>
    <div id="error_explanation">
      <h2>Não foi possível cadastradar sua conta:</h2>

      <ul>
        <% @psicologo.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div> 
    <%= f.label :crp%>
    <%= f.text_field :crp%><br>
  </div>
  <div> 
    <%= f.label :nome%>
    <%= f.text_field :nome%><br>
  </div>
  <div> 
    <%= f.label :senha%>
    <%= f.password_field :password%><br>
  </div>

  <%= f.submit %>
<%end%>
